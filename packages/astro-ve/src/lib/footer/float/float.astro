---
export interface Props {
	data?: string;
	widget?: string;
}

const { data, widget } = Astro.props;
---

<div
	class="fixed bottom-0 left-0 z-20 w-full h-16 border-t bg-stone-900/75 border-gray-600 p-1 transform transition-transform ease-in-out duration-500">
	<div class="grid h-full max-w-lg grid-cols-4 mx-auto font-medium">
		<slot name="raw" />
		<slot />
	</div>
</div>
